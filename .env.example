# SLURM Configuration for MeluXina cluster
# Copy this file to .env and update with your values

# Your SLURM account (required for job submission)
SLURM_ACCOUNT=your_account_name

# Partition to use (default: cpu)
SLURM_PARTITION=cpu

# Quality of Service
SLURM_QOS=default

# Time limit for jobs (format: HH:MM:SS)
SLURM_TIME_LIMIT=00:30:00
